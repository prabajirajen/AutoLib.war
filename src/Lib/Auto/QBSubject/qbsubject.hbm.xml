<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">



<hibernate-mapping package="Lib.Auto.QBSubject">

	<class name="QBsubjectbean" table="qbsubject_mas" lazy="false">
		<id name="qbcode" column="qb_code" type="int">
			<generator class="assigned" />
		</id>
		<property name="qbsubcode" column="qbsub_code" type="java.lang.String" />
		<property name="qbsubname" column="qbsub_name" type="java.lang.String" />
		<property name="qbsubdesc" column="qbsub_desc" type="java.lang.String" />
		<property name="qbbranchcode" column="branch_code" type="int" />

	</class>

</hibernate-mapping>
