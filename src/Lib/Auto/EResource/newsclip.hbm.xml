<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="Lib.Auto.EResource">

<class name="NewscllipingBean" table="newsclipping_mas" lazy="false">
		<id name="ncode" column="Newspaper_code" type="int">
			<generator class="assigned"/>
	    </id>
	   <property name="nname" column="Newspaper_name" type="java.lang.String"/>
		<property name="ntype" column="Newspaper_type" type="java.lang.String"/>  
		<property name="ndate" column="Newspaper_ndate" type="java.lang.String"/>  
		<property name="pno" column="Newspaper_pno" type="java.lang.String"/> 
		<property name="ntitle" column="Newspaper_title" type="java.lang.String"/> 
		<property name="nsubject" column="Newspaper_subject" type="java.lang.String"/> 
		<property name="nkey" column="Newspaper_key" type="java.lang.String"/> 
		<property name="nabstract" column="Newspaper_abstract" type="java.lang.String"/> 
		<property name="ncontent" column="Newspaper_content" type="java.lang.String"/> 
	
	
		
</class>






</hibernate-mapping>



