<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="Lib.Auto.EResource">
<class name="EResourceBean" table="web_mas" lazy="false">
		<id name="ecode" column="code" type="int">
			<generator class="assigned"/>
	    </id>
	    <property name="esite" column="website" type="java.lang.String"/>
		<property name="edetails" column="webdetails" type="java.lang.String"/> 
		<property name="etitle" column="webtitle" type="java.lang.String"/> 
		<property name="esubtitle" column="websubtitle" type="java.lang.String"/> 
		<property name="type" column="webtype" type="java.lang.String"/> 
		<property name="branchCode" column="branch_code" type="int"/> 
		
		
		







	
</class>

</hibernate-mapping>



