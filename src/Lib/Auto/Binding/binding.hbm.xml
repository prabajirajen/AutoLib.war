<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="Lib.Auto.Binding">

<class name="BindingBean" table="binder_mas" lazy="false">
		<id name="code" column="binder_code" type="int">
			<generator class="assigned"/>
	    </id>
	   <property name="name" column="binder_name" type="java.lang.String"/>
		<property name="desc" column="short_desc" type="java.lang.String"/>  
		<property name="add1" column="address1" type="java.lang.String"/>  
		 <property name="add2" column="address2" type="java.lang.String"/>
		<property name="city" column="city" type="java.lang.String"/>  
		<property name="state" column="state" type="java.lang.String"/> 
		<property name="country" column="country" type="java.lang.String"/>
		<property name="pin" column="pincode" type="java.lang.String"/>  
		<property name="phone" column="phone" type="java.lang.String"/> 
		<property name="fax" column="fax" type="java.lang.String"/> 
		<property name="email" column="emailid" type="java.lang.String"/>
		<property name="url" column="urlid" type="java.lang.String"/>
		
		
</class>






</hibernate-mapping>



