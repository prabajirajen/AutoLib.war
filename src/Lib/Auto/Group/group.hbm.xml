<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="Lib.Auto.Group">

<class name="GroupBean" table="group_mas" lazy="false">
		<id name="code" column="Group_Code" type="int">
			<generator class="assigned"/>
	    </id>
	   <property name="name" column="Group_Name" type="java.lang.String"/>
		<property name="geli" column="GEligible" type="int"/>  
		<property name="beli" column="BEligible" type="int"/>
		<property name="bbeli" column="BBEligible" type="int"/>  		
  		<property name="bveli" column="BVEligible" type="int"/>
    	<property name="nbeli" column="NBEligible" type="int"/>
      	<property name="jeli" column="JEligible" type="int"/>
        <property name="seli" column="SEligible" type="int"/>
		<property name="reli" column="REligible" type="int"/>
		<property name="peli" column="PEligible" type="int"/>
		<property name="teli" column="TEligible" type="int"/>
		<property name="glper" column="GLPeriod" type="int"/>
		<property name="blper" column="BLPeriod" type="int"/>
		<property name="bblper" column="BBLPeriod" type="int"/>
		<property name="bvlper" column="BVLPeriod" type="int"/>
		<property name="nblper" column="NBLPeriod" type="int"/>
		<property name="jlper" column="JLPeriod" type="int"/>
		<property name="slper" column="SLPeriod" type="int"/>		
		<property name="rlper" column="RLPeriod" type="int"/>
		<property name="plper" column="PLPeriod" type="int"/>
		<property name="tlper" column="TLPeriod" type="int"/>
		<property name="remarks" column="Remarks" type="java.lang.String"/>
		<property name="renewal" column="Renewal" type="int"/>
		<property name="status" column="STATUS" type="java.lang.String"/>
		<property name="branchCode" column="branch_code" type="int"/>

</class>

   

</hibernate-mapping>



