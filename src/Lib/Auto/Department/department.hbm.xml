<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="Lib.Auto.Department">

<class name="DepartmentBean" table="department_mas" lazy="false">
		<id name="code" column="dept_code" type="int">
			<generator class="assigned"/>
	    </id>
	    <property name="name" column="dept_name" type="java.lang.String"/>
		<property name="desc" column="short_desc" type="java.lang.String"/>  
		<property name="branchCode" column="branch_code" type="int"/> 
		
</class>
</hibernate-mapping>



