<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">



<hibernate-mapping package="Lib.Auto.Subject">

<class name="subjectbean" table="subject_mas" lazy="false">
		<id name="code" column="sub_code" type="int">
			<generator class="assigned"/>
	    </id>
	   <property name="name" column="sub_name" type="java.lang.String"/>
	   <property name="callno" column="Call_No" type="java.lang.String"/>		   
       <property name="desc" column="Short_Desc" type="java.lang.String"/>  
	   <property name="branchCode" column="branch_code" type="int"/> 	
		
</class>

</hibernate-mapping>
