<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="Lib.Auto.Browse">

<sql-query name="browseSearchQuery">

	SELECT  access_no,call_no,title,author_name,location,availability,document,branch_name FROM full_search where 2>1

</sql-query>


</hibernate-mapping>



