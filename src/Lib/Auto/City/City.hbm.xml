<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="Lib.Auto.City">

<class name="CityBean" table="City_mas" lazy="false">
		<id name="code" column="city_code" type="int">
			<generator class="assigned"/>
	    </id>
	   <property name="name" column="City_name" type="java.lang.String"/>
	   <property name="state" column="State" type="java.lang.String"/> 
	   <property name="country" column="Country" type="java.lang.String"/> 
	   <property name="pincode" column="Pincode" type="int"/> 
	   <property name="desc" column="short_desc" type="java.lang.String"/>  
		
				
</class>
</hibernate-mapping>



